@article{MiniBooNECollaboration2018,
abstract = {The MiniBooNE experiment at Fermilab reports results from an analysis of {\$}\backslashnu{\_}e{\$} appearance data from {\$}12.84 \backslashtimes 10{\^{}}{\{}20{\}}{\$} protons on target in neutrino mode, an increase of approximately a factor of two over previously reported results. A {\$}\backslashnu{\_}e{\$} charged-current quasi-elastic event excess of {\$}381.2 \backslashpm 85.2{\$} events ({\$}4.5 \backslashsigma{\$}) is observed in the energy range {\$}200{\textless}E{\_}\backslashnu{\^{}}{\{}QE{\}}{\textless}1250{\$}{\~{}}MeV. Combining these data with the {\$}\backslashbar \backslashnu{\_}e{\$} appearance data from {\$}11.27 \backslashtimes 10{\^{}}{\{}20{\}}{\$} protons on target in antineutrino mode, a total {\$}\backslashnu{\_}e{\$} plus {\$}\backslashbar \backslashnu{\_}e{\$} charged-current quasi-elastic event excess of {\$}460.5 \backslashpm 95.8{\$} events ({\$}4.8 \backslashsigma{\$}) is observed. If interpreted in a standard two-neutrino oscillation model, {\$}{\{}\backslashnu{\}}{\_}{\{}\backslashmu{\}} \backslashrightarrow {\{}\backslashnu{\}}{\_}e{\$}, the best oscillation fit to the excess has a probability of {\$}20.1\backslash{\%}{\$} while the background-only fit has a {\$}\backslashchi{\^{}}2{\$}-probability of {\$}5 \backslashtimes 10{\^{}}{\{}-7{\}}{\$} relative to the best fit. The MiniBooNE data are consistent in energy and magnitude with the excess of events reported by the Liquid Scintillator Neutrino Detector (LSND), and the significance of the combined LSND and MiniBooNE excesses is {\$}6.1 \backslashsigma{\$}. All of the major backgrounds are constrained by in-situ event measurements, so non-oscillation explanations would need to invoke new anomalous background processes. Although the data are fit with a standard oscillation model, other models may provide better fits to the data.},
